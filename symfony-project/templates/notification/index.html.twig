<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<meta name="author" content="Untree.co">
		<link rel="shortcut icon" href="https://cdn0.iconfinder.com/data/icons/job-seeker/256/choose_job_seeker_employee_unemployee_work-512.png">

		<meta name="description" content=""/>
		<meta name="keywords" content="Boostrap"/>


		<link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;700&display=swap" rel="stylesheet">

		<link rel="stylesheet" href="css/bootstrap.min.css">
		<link rel="stylesheet" href="css/owl.carousel.min.css">
		<link rel="stylesheet" href="css/owl.theme.default.min.css">
		<link rel="stylesheet" href="fonts/icomoon/style.css">
		<link rel="stylesheet" href="fonts/feather/style.css">
		<link rel="stylesheet" href="fonts/flaticon/font/flaticon.css">
		<link rel="stylesheet" href="css/jquery.fancybox.min.css">
		<link rel="stylesheet" href="css/aos.css">
		<link rel="stylesheet" href="css/style.css">

		<title>AdopteUnDev</title>
	</head>
	<body>
		<div class="container w-100" style="margin-bottom:5rem;width:100vw">
			<nav class="site-nav" style="width:100vw;">
				<div class="logo">
					<a class="text-black">AdopteUnDev<span class="text-black">.</span>
					</a>
				</div>
				<div class="row align-items-center">

					<div class="col-12 col-sm-12 col-lg-12 site-navigation text-center">
						<ul class="js-clone-nav d-none d-lg-inline-block text-left site-menu">
							<li>
								<a class="text-black" href="{{path('app_developer')}}">Accueil</a>
							</li>
							<li class="active">
								<a class="text-black" href="{{path('app_offre')}}">Offres</a>
							</li>
							<li class="active">
								<a class="text-black" href="{{path('app_matching')}}">Matching</a>
							</li>
							<li>
								<a class="text-black font-weight-bold" href="{{ path('app_notifications') }}">
									Notifications
									<span class="icon-notifications"></span>
								</a>
							</li>
							<li>
								<a class="text-black" href="{{path('app_favori')}}">Favoris</a>
							</li>
							<li class="has-children">
								<a href="#" class="text-black">Mon compte</a>
								<span class="icon-user"></span>
								<ul class="dropdown">
									<li>
										<a class="text-black" style="cursor:pointer;" href="{{path('app_developer_login')}}">Connexion</a>
									</li>
									<li>
										<a class="text-black" style="cursor:pointer;" href="{{path('app_inscription')}}">Inscription</a>
									</li>
								</ul>
							</li>
						</ul>

					</div>

				</div>
			</nav>
		</div>
		<div class="site-section bg-light d-flex min-vh-100">
			<div class="container">
				<div class="row">
					<h2 class="font-weight-bold text-center text-primary">Vos notifications</h2>
				</div>

				<div class="container">
					<ul class="list-group">
						{% for notification in notifications %}
							<li class="list-group-item d-flex justify-content-between align-items-center">
								<div>
									<strong>{{ notification.message }}</strong><br>
								</div>
								<a href="{{ path('app_detail', {'id': notification.job.id}) }}" class="btn btn-success btn-sm">
									Voir l'offre
								</a>
							</li>
						{% else %}
							<li class="list-group-item text-center">
								Aucune notification pour le moment.
							</li>
						{% endfor %}
					</ul>
				</div>
			</div>
		</div>

	</body>
</html></div><div id="overlayer"></div><div class="loader">
